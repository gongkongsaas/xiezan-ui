{
  "name": "assistech-vue",
  "version": "0.1.0",
  "private": false,
  "description": "vue3结合typescript组件库",
  "author": {
    "name": "iworkerweb",
    "email": "865116277@qq.com",
    "url": "https://github.com/iworkerweb/assistech-vue"
  },
  "license": "MIT",
  "keywords": [
    "vue3",
    "typescript",
    "组件库",
    "协赞"
  ],
  "homepage": "https://github.com/iworkerweb/assistech-vue",
  "repository": {
    "type": "git",
    "url": "https://github.com/iworkerweb/assistech-vue"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-ts": "tsc -p tsconfig.build.json",
    "build-css": "lessc ./src/styles/index.less ./dist/index.css",
    "clean": "rimraf ./dist",
    "build:lib": "vue-cli-service build --target lib --name myLib ./src/index.tsx"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "core-js": "^3.6.5",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.0.0-0",
    "less": "^4.0.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "typescript": "~3.9.3",
    "vue": "^3.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
